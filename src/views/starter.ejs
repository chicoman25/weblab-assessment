<html>
    <head>
        <title>Static Content</title>
    </head>
    <body>
        <h1> <%= title %> </h1>
        <p>
            <%= date %>
        </p>
        <p>
            <ul> 
                <% for(i=0; i < list.length; i++){ %>
                    <li> <%= list[i] %> </li>
                <% } %>
            </ul>
        </p>
    </body>
</html>